<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="defaultBinding">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://localhost:4711/tracing"
             binding="netTcpBinding"
             bindingConfiguration="defaultBinding"
             contract="Billapong.Contract.Service.ITracingService" />
      <endpoint address="net.tcp://localhost:4711/mapeditor"
             binding="netTcpBinding"
             bindingConfiguration="defaultBinding"
             contract="Billapong.Contract.Service.IMapEditorService" />
      <endpoint address="net.tcp://localhost:4711/session"
             binding="netTcpBinding"
             bindingConfiguration="defaultBinding"
             contract="Billapong.Contract.Service.ISessionService" />
    </client>
  </system.serviceModel>
</configuration>